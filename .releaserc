{
    "branches": [
        "main"
    ],
    "repositoryUrl": "https://github.com/RedFroggy/keycloak-event-listener-sns",
    "plugins": [
        [
            "@semantic-release/github",
            {
                "assets": [
                    {
                        "label": "jar"
                    }
                ]
            }            
        ],
        [
            "@semantic-release/exec",
            {
                "verifyReleaseCmd": "echo \"NEXT_RELEASE_VERSION=${nextRelease.version}\" >> $GITHUB_ENV"
            }
        ]
    ]
}